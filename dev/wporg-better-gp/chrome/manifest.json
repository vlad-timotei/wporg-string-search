{
   "manifest_version":2,
   "name":"Better GlotPress",
   "version":"1.0",
   "description":"Custom GP Settings",
   "permissions":[
      "activeTab",
      "tabs",
      "storage"
   ],
   "content_scripts":[
      {
         "matches":[
            "https://translate.wordpress.org/*"
         ],
         "css":[
            "wporg-better-gp.css"
         ],
         "js":[
            "jquery-3.6.0.min.js",
            "wporg-better-gp.js"
         ]
      }
   ]
}